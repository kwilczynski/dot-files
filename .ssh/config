# To add Github's keys, run the following:
#   ssh-keyscan github.com >> ~/.ssh/known_hosts
Host github.com *.github.com
  CheckHostIP no
  StrictHostKeyChecking yes
  ControlMaster no
  ControlPath none
  LogLevel ERROR

#Host *.amazonaws.com
#  User ubuntu
#  IdentityFile /dev/null
