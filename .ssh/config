Host *
  ServerAliveInterval 60
  ForwardAgent yes
  IdentitiesOnly yes
  CheckHostIP yes

Host github.com *.github.com
  CheckHostIP no
  StrictHostKeyChecking yes
  LogLevel ERROR

Host *.amazonaws.com
  User ubuntu
  IdentityFile ~/.ssh/file.pem
  UserKnownHostsFile /dev/null
  CheckHostIP no
  StrictHostKeyChecking no
  LogLevel INFO
