[alias]
	a = add
	t = tag
	au = add -u .
	ap = add -p
	br = branch -v
	bl = blame
	sh = show
	sb = show-branch
	ds = diff --stat
	ls = ls-files
	lo = log
	st = status
	co = checkout
	cp = cherry-pick
	ci = commit
	df = diff
	rb = rebase
	rbc = rebase --continue
	rbs = rebase --skip
	rs = remote show
	sb = show-branch
	pu = pull
	pt = push --tags
	w = whatchanged
	dfs = diff --staged
	sts = status -s
	rl = reflog
	ref = reflog
	rst = reset
	rt = remote
	rup = remote update
	rpr = remote prune
	rbi = rebase -i
	cia = commit -a
	com = checkout master
	detach = checkout HEAD^0
	info = config --list
	summary = log --oneline
	rank = shortlog -sn --no-merges
	tree = log --graph --oneline --decorate
	pom = push origin master
	pob = push origin `__git_current_branch`
	amend = commit --amend
	last = log --oneline -10
	review = log --stat=160,180 -p -w --patience --reverse -M -C -C -c

[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
	ui = true
	pager = true

[pack]
	threads = 0

[rename]
	limit = 0

[core]
	pager = less -FRSX

[push]
	default = current

[giggle]
	main-window-maximized = true
	main-window-view = HistoryView

[sendemail]
	smtpuser =
	smtppass =
	smtpserver =
	smtpserverport =
	smtpencryption = tls
	suppresscc = self
	signedoffbycc = false
	chainreplyto = false
